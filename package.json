{
  "name": "nextjs-ci-tools",
  "version": "1.0.0",
  "repository": "git@github.com:StarpTech/next-localization.git",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "prebuild": "npm i --save react@npm:@preact/compat react-dom@npm:@preact/compat",
    "build": "next build",
    "build:ssg": "cross-env NODE_ENV=production next build && next export -o build",
    "build:ssr": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "serve:lhci": "npm run start",
    "bundlewatch": "bundlewatch || true",
    "vercel-build": "next build",
    "lint": "eslint . --fix",
    "linter": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier-standard --format"
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@axe-core/react": "4.1.0",
    "@babel/core": "7.12.10",
    "@bundle-analyzer/webpack-plugin": "0.5.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@cypress/code-coverage": "3.9.1",
    "@next/bundle-analyzer": "10.0.5",
    "@next/eslint-plugin-next": "10.0.5",
    "@relative-ci/agent": "1.4.0",
    "@testing-library/react": "11.2.3",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.36",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "babel-jest": "26.6.3",
    "bundlewatch": "0.3.1",
    "commitizen": "4.2.2",
    "cross-env": "7.0.3",
    "cypress": "6.2.1",
    "dependency-version-badge": "1.9.1",
    "eslint": "7.17.0",
    "eslint-config-preact": "1.1.3",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "prettier-standard": "16.4.1",
    "standard-version": "9.1.0",
    "start-server-and-test": "1.11.7",
    "typedoc": "0.20.14",
    "typescript": "4.1.3",
    "webpack-stats-plugin": "1.0.3"
  },
  "dependencies": {
    "@sentry/webpack-plugin": "1.14.0",
    "@zeit/next-source-maps": "0.0.3",
    "next": "10.0.5",
    "next-compose-plugins": "2.2.1",
    "next-plugin-bundle-stats": "2.5.0",
    "next-plugin-preact": "3.0.3",
    "preact": "10.5.10",
    "preact-render-to-string": "5.1.12",
    "react": "npm:@preact/compat@0.0.4",
    "react-dom": "npm:@preact/compat@0.0.4",
    "react-ssr-prepass": "npm:preact-ssr-prepass@1.1.3"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "bundlewatch": {
    "trackBranches": [
      "master"
    ],
    "files": [
      {
        "path": ".next/static/chunks/*.js",
        "maxSize": "4MB"
      }
    ]
  },
  "license": "UNLICENSED"
}
